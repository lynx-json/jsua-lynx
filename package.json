{
  "name": "jsua-lynx",
  "version": "0.1.32",
  "description": "JSUA extensions for the Lynx media type.",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test-temp": "browserify tape.js | smokestack --saucelabs",
    "test": "mocha test --recursive --compilers js:babel-register --require babel-polyfill",
    "pretest": "npm run build",
    "build": "babel node_modules/jsua/src -d node_modules/jsua/lib && babel src -d lib"
  },
  "keywords": [
    "lynx",
    "lynx+json",
    "jsua"
  ],
  "author": "lynx-json.org",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "browserify": "^14.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "smokestack": "^3.4.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "jsua": "github:lynx-json/jsua",
    "lynx-parser": "github:lynx-json/lynx-parser"
  }
}
